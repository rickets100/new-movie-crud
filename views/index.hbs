<div class="container">
  <h1>Movies Index Page</h1>
  {{#each movies}}
  <div class="row">
    <div class="col-md-2">
      <img src="{{ this.poster_url }}"
        alt="{{ this.title }}"
        class="poster">
    </div>
    <div class="col-md-6">
      <h2>{{ this.title }}</h2>
      <p>Director: {{ this.director }}</p>
      <p>Year: {{ this.year }}</p>
      <p>My Rating: {{ this.my_rating }}</p>
    </div>
  </div>
  {{/each}}
</div>


